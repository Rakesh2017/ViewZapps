<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    android:id="@+id/fragment_container_dashboard"
    tools:context="enhabyto.com.viewzapps.DashBoard"
    android:background="@color/whiteSmoke"
    tools:showIn="@layout/app_bar_dash_board">


    <!--vertical divider-->
    <android.support.v4.widget.Space
        android:layout_height="match_parent"
        android:layout_width="1dp"
        android:layout_centerInParent="true"
        android:id="@+id/dash_dividerVertical"
        />


    <!--linear layout 1-->
    <LinearLayout
        android:id="@+id/dash_linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/dash_dividerVertical"
        android:orientation="vertical"
        android:weightSum="3"
        >

        <!--youtube-->
        <ImageButton
            android:id="@+id/dash_youtubeButton"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/youtube"
            android:layout_weight="1"
            tools:ignore="ContentDescription"
            />


        <!--instagram-->
        <ImageButton
            android:id="@+id/dash_instagramButton"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/instagram"
            android:layout_weight="1"
            tools:ignore="ContentDescription"/>

        <!--apple-->
        <ImageButton
            android:id="@+id/dash_appleButton"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/apple"
            android:layout_weight="1"
            tools:ignore="ContentDescription"
            />


    </LinearLayout>



    <!--linear layout 2-->
    <LinearLayout
        android:id="@+id/dash_linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/dash_dividerVertical"
        android:orientation="vertical"
        android:weightSum="3"
        >

        <!--facebook-->
        <ImageButton
            android:id="@+id/dash_facebookButton"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/facebook"
            android:layout_weight="1"
            tools:ignore="ContentDescription"
            />

        <!--android-->
        <ImageButton
            android:id="@+id/dash_androidButton"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/android"
            android:layout_weight="1"
            tools:ignore="ContentDescription"
            />

        <!--movies-->
        <ImageButton
            android:id="@+id/dash_moviesButton"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/movies"
            android:layout_weight="1"
            tools:ignore="ContentDescription" />


    </LinearLayout>




</RelativeLayout>
